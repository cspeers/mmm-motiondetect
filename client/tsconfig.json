{
    "compilerOptions": {
        "target": "ES6",
        "noImplicitAny": true,
        "allowSyntheticDefaultImports": true,
        "moduleResolution": "Node",
        "sourceMap": true,
        "alwaysStrict": true,
        "diagnostics": false,
        "listFiles": false,
        "noEmitOnError": true,
        "composite": true,
        "outDir": "../",
        "baseUrl": "../",
        "typeRoots": [
            "../node_modules/@types",
            "../@types/"
        ]
    },
    "exclude": [

    ],
    "include": [
        "./*"
    ]
}